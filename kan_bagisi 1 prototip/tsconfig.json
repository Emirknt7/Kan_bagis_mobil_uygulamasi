{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "index.tsx",  // Bu satırı düzeltin
    "app/(tabs)/profileScreen.js"  // Bu satırı düzeltin
  ]
}
